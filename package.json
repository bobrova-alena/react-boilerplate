{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "main": "dist/server/server.js",
  "description": "React boilerplate",
  "author": "Bobrova Alena",
  "license": "MIT",
  "dependencies": {
    "@loadable/component": "5.15.2",
    "@redux-devtools/extension": "3.2.2",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "lodash": "4.17.21",
    "nodemon-webpack-plugin": "4.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-number-format": "4.9.3",
    "react-redux": "8.0.2",
    "react-router-dom": "6.3.0",
    "react-svg": "15.1.3",
    "redux-saga": "1.1.3",
    "styled-components": "5.3.5",
    "styled-system": "5.1.5"
  },
  "scripts": {
    "clean": "rimraf dist",
    "watch": "npm-run-all --parallel --print-label watch:client:dev watch:server:dev",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build": "npm-run-all --print-label build:client:prod build:server:prod",
    "build:dev": "npm-run-all --print-label build:client:dev build:server:dev",
    "watch:prod": "npm-run-all --parallel --print-label watch:client:prod watch:server:prod",
    "run:nodemon": "nodemon dist/server/server.js",
    "lint:styles": "stylelint \"./src/**/*.ts?(x)\" --fix",
    "lint:code": "eslint \"./src/**/*.ts?(x)\" --fix",
    "------------": "",
    "watch:client:dev": "cross-env MODE_ENV=development webpack --progress --color --mode development --watch --config ./webpack/webpack.client.js",
    "watch:server:dev": "cross-env MODE_ENV=development SSL_CERTS_CHECK_MODE_ENV=suppress webpack --progress --color --mode development --watch --config ./webpack/webpack.server.js",
    "watch:client:prod": "webpack --progress --color --mode production --watch --config ./webpack/webpack.client.js",
    "watch:server:prod": "cross-env SSL_CERTS_CHECK_MODE_ENV=suppress webpack --progress --color --mode production --watch --config ./webpack/webpack.server.js",
    "build:client:dev": "cross-env MODE_ENV=development webpack --progress --color --mode development --config ./webpack/webpack.client.js",
    "build:server:dev": "cross-env MODE_ENV=development webpack --progress --color --mode development --config ./webpack/webpack.server.js",
    "build:client:prod": "webpack --color --mode production --config ./webpack/webpack.client.js",
    "build:server:prod": "webpack --color --mode production --config ./webpack/webpack.server.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.18.6",
    "@reduxjs/toolkit": "1.8.3",
    "@storybook/addon-actions": "6.5.9",
    "@storybook/addon-essentials": "6.5.9",
    "@storybook/addon-interactions": "6.5.9",
    "@storybook/addon-links": "6.5.9",
    "@storybook/react": "6.5.9",
    "@storybook/testing-library": "0.0.13",
    "@types/express": "4.17.13",
    "@types/loadable__component": "5.13.4",
    "@types/lodash": "4.14.182",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@types/react-json-editor-ajrm": "2.5.3",
    "@types/react-redux": "7.1.24",
    "@types/styled-components": "5.1.25",
    "@types/styled-system": "^5.1.15",
    "@types/webpack-node-externals": "2.5.3",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "babel-loader": "8.2.5",
    "circular-dependency-plugin": "5.2.2",
    "cross-env": "7.0.3",
    "css-loader": "6.7.1",
    "css-minimizer-webpack-plugin": "4.0.0",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-webpack-plugin": "3.2.0",
    "fork-ts-checker-webpack-plugin": "7.2.11",
    "html-webpack-plugin": "5.5.0",
    "mini-css-extract-plugin": "2.6.1",
    "nodemon": "2.0.18",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "stylelint": "14.9.1",
    "stylelint-config-recommended": "8.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "stylelint-webpack-plugin": "3.3.0",
    "ts-loader": "9.3.1",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "typescript": "4.7.4",
    "typescript-plugin-styled-components": "2.0.0",
    "webpack": "5.73.0",
    "webpack-cli": "4.10.0",
    "webpack-stream": "7.0.0"
  }
}
